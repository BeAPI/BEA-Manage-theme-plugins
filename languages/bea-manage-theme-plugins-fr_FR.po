#: compat.php:22
#, php-format
msgid "Plugin BEA - Manage theme plugins requires PHP version %s or greater to be activated."
msgstr "Le Plugin BEA - Manage theme plugins requiert la version PHP %s ou plus pour être activé."

#: compat.php:37
#, php-format
msgid ""
"Plugin BEA - Manage theme plugins require PHP version %s or greater to be activated. Your "
"server is currently running PHP version %s."
msgstr ""
"Le plugin BEA - Manage theme plugins requiert la version PHP %s ou plus pour être activé. Votre "
"serveur utilise actuellement la version %s de PHP."

#: classes/admin/main.php:170
#, php-format
msgid "The plugin %s is not installed in your WordPress instance."
msgstr "L'extension %s n'est pas installée dans votre instance WordPress."

#. Name of the plugin
msgid "BEA - Manage theme plugins"
msgstr "BEA - Manage theme plugins"

#. URI of the plugin
#. Author URI of the plugin
