#, php-format
msgid "Plugin Boilerplate requires PHP version %s or greater to be activated."
msgstr "Plugin Boilerplate requires PHP version %s or greater to be activated."

#: compat.php:37
#, php-format
msgid ""
"Plugin Boilerplate require PHP version %s or greater to be activated. Your "
"server is currently running PHP version %s."
